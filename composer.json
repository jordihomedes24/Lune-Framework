{
    "name": "jordihomedes24/lune-framework",
    "description": "PHP framework inspired on Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lune\\": "src/"
        },
        "files": [
            "src/Helpers/app.php",
            "src/Helpers/http.php",
            "src/Helpers/string.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lune\\Tests\\": "tests"
        }
    },
    "scripts": {
        "tests": "phpunit --color=always --testdox tests/",
        "fixer": "php-cs-fixer fix --diff"
    },
    "authors": [
        {
            "name": "jordihomedes24",
            "email": "jordihomedes48@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "*",
        "friendsofphp/php-cs-fixer": "^3.34"
    }
}
